{
    "name": "FNS searcher Extensions",
    "description": "Underground Level Extension",
    "version": "0.0.1",
    "manifest_version": 3,
    "permissions": [
        "activeTab",
        "scripting",
        "tabs",
        "background",
        "notifications"
    ],
    "host_permissions": [
        "https://*/*",
        "http://*/*"
    ],
    "action": {
        "default_popup": "./controll/index.html",
        "default_icon": {
            "48": "./assets/icons.png"
        }
    },
    "background": {
        "service_worker": "app/index.js",
        "type": "module"
    }
}